{
  "name": "accessor-fn",
  "version": "1.2.2",
  "description": "A wrapper function for object accessor properties",
  "unpkg": "dist/accessor-fn.min.js",
  "main": "dist/accessor-fn.common.js",
  "module": "dist/accessor-fn.module.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/accessor-fn.git"
  },
  "keywords": [
    "accessor",
    "web",
    "component",
    "wrapper"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "http://bl.ocks.org/vasturiano"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vasturiano/accessor-fn/issues"
  },
  "homepage": "https://github.com/vasturiano/accessor-fn",
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rimraf dist && mkdir dist && rollup -c",
    "minify": "terser dist/accessor-fn.js -o dist/accessor-fn.min.js -c -m --comments '/Version/'",
    "build-module": "rollup -c rollup.config.module.js",
    "prepare": "npm run build && npm run minify && npm run build-module"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.0.6",
    "rollup-plugin-babel": "^4.4.0",
    "terser": "^4.6.6"
  }
}
